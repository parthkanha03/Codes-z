#include <iostream>
#include<bits/stdc++.h>
#include <deque> 
using namespace std;

void printKMax(int arr[], int n, int k){
	//Write your code here.
    deque<int> dq;
    vector<int> v(n-k+1);
    for(int i=0 ; i< n ;++i){
        //pop_back() pop_front push_back push_front
        
        if(!dq.empty() && dq.front()<=i-k){
            dq.pop_front();
        }
        while(!dq.empty() && arr[i]>arr[dq.back()]){
            dq.pop_back();
        }
        
        dq.push_back(i);
        if(i>=(k-1)){
            cout<<arr[dq.front()]<<" ";
        }
        
    }
    cout<<endl;
    
}

int main(){
  
	int t;
	cin >> t;
	while(t>0) {
		int n,k;
    	cin >> n >> k;
    	int i;
    	int arr[n];
    	for(i=0;i<n;i++)
      		cin >> arr[i];
    	printKMax(arr, n, k);
    	t--;
  	}
  	return 0;
}
